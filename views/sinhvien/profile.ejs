<!DOCTYPE html>
<html lang="en">
<head>
    <title>Material Admin - Dynamic tables</title>

    <!-- BEGIN META -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="your,keywords">
    <meta name="description" content="Short explanation about this website">
    <!-- END META -->


    <!-- BEGIN STYLESHEETS -->
    <link href='http://fonts.googleapis.com/css?family=Roboto:300italic,400italic,300,400,500,700,900' rel='stylesheet'
          type='text/css'/>
    <link type="text/css" rel="stylesheet" href="/assets/css/theme-default/bootstrap.css?1422792965"/>
    <link type="text/css" rel="stylesheet" href="/assets/css/theme-default/materialadmin.css?1425466319"/>
    <link type="text/css" rel="stylesheet" href="/assets/css/theme-default/font-awesome.min.css?1422529194"/>
    <link type="text/css" rel="stylesheet"
          href="/assets/css/theme-default/material-design-iconic-font.min.css?1421434286"/>
    <link type="text/css" rel="stylesheet"
          href="/assets/css/theme-default/libs/DataTables/jquery.dataTables.css?1423553989"/>
    <link type="text/css" rel="stylesheet"
          href="/assets/css/theme-default/libs/DataTables/extensions/dataTables.colVis.css?1423553990"/>
    <link type="text/css" rel="stylesheet"
          href="/assets/css/theme-default/libs/DataTables/extensions/dataTables.tableTools.css?1423553990"/>

    <link href="/stylesheets/upload.css" rel="stylesheet">
    <!-- END STYLESHEETS -->


    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script type="text/javascript" src="/assets/js/libs/utils/html5shiv.js?1403934957"></script>
    <script type="text/javascript" src="/assets/js/libs/utils/respond.min.js?1403934956"></script>
    <![endif]-->
</head>
<body class="menubar-hoverable header-fixed ">
<%- include('../header') %>
<!-- BEGIN BASE-->
<div id="base">
    <!-- BEGIN CONTENT-->
    <div id="content">
        <section>
            <div class="section-body content-lg">
                <div class="row">
                    <div class="col-md-3">
                        <div class="card">
                            <div class="card-body">
                                <img class="img-circle border-white border-xl img-responsive profile-avatar col-centered"
                                     src="/images/gv.gif" alt="">
                                <h3 class="text-center"><%= user.tenNguoiDung %><br>
                                    <small>Computer Sicence</small>
                                </h3>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-9">
                        <div class="card">
                            <div class="card-head">
                                <ul class="nav nav-tabs" data-toggle="tabs">
                                    <li class="active"><a href="#tab-about">About</a></li>
                                    <li><a href="#tab-edit">Edit</a></li>
                                    <li><a href="#tab-edit1">Dang ky de tai</a></li>
                                    <li><a href="#tab-edit2">Dang ky bao ve</a></li>
                                </ul>
                            </div>
                            <div class="card-body tab-content">
                                <div class="tab-pane active" id="tab-about">
                                    <div class="about-area">
                                        <h4 class="text-default-light">Thông tin cá nhân</h4>
                                        <div class="table-responsive">
                                            <table class="table about-table">
                                                <tbody>
                                                <tr>
                                                    <th>Ma sinh vien</th>
                                                    <td><%= nguoihoc.MSSV %></td>
                                                </tr>
                                                <tr>
                                                    <th>User Name</th>
                                                    <td><%= user.tenNguoiDung %></td>
                                                </tr>
                                                <tr>
                                                    <th>Dia chi mail vnu</th>
                                                    <td><%= user.vnuMail %></td>
                                                </tr>
                                                <tr>
                                                    <th>Mat Khau</th>
                                                    <td><%= user.matKhau %></td>
                                                </tr>
                                                <tr>
                                                    <th>Dang Ky Khoa Luan</th>

                                                </tr>
                                                <tr>
                                                    <th>Chuyên ngành</th>
                                                    <td><%= nguoihoc.nghanhHoc_khNganh %></td>
                                                </tr>
                                                <tr>
                                                    <th>Website</th>
                                                    <td><a href="#">UET.vnu.edu.vn</a></td>
                                                </tr>
                                                <tr>
                                                    <th>Email</th>
                                                    <td><a href="#">Ctmail.vnu.edu.vn</a></td>
                                                </tr>
                                                <tr>
                                                    <th>Phone</th>
                                                    <td>+61 3 8376 6284</td>
                                                </tr>
                                                <tr>
                                                    <th>Position</th>
                                                    <td>Xuan Thuy,Cau Giay, Ha Noi</td>
                                                </tr>
                                                </tbody>
                                            </table>
                                        </div>

                                    </div><!--  end-aboutarea -->
                                    <% if(detai){ %>
                                    <div class="about-area">
                                        <h4 class="text-default-light">Đề tài đã dăng ký</h4>
                                        <dl class="table-responsive">
                                            <table class="table about-table">
                                                <tbody>
                                                <tr>
                                                    <th>Mã đề tài</th>
                                                    <td><%= detai.ID %></td>
                                                </tr>
                                                <tr>
                                                    <th>Tên đề tài</th>
                                                    <td><%= detai.tenDeTai %></td>
                                                </tr>
                                                <th>GV hướng dẫn</th>
                                                <td><%= detai.giangVien_ID %></td>
                                                </tbody>
                                            </table>
                                        </dl>
                                    </div>
                                    <% } %>
                                </div><!--  tab-about -->

                                <div class="tab-pane" id="tab-edit1">
                                    <% if(detai){ %>
                                    <% if(detai.choDuocDuyet == 1){ %>
                                    <div class="alert alert-callout alert-warning" role="alert">
                                        Bạn đã xin hủy đề tài, Yêu cầu của bạn đã được gửi đến khoa
                                    </div>
                                    <% }else if(detai.choDuocDuyet == 2){ %>
                                    <div class="alert alert-callout alert-warning" role="alert">
                                        Bạn đã xin sửa đề tài, Yêu cầu của bạn đã được gửi đến khoa
                                    </div>
                                    <% } %>
                                    <button type="button" id="huy-detai" class="btn btn-default-bright btn-block">Huỷ đề
                                        tài
                                    </button>
                                    <% } %>
                                    <form class="form" method="post" action="/users/sinhvien/dkdetai"
                                          novalidate="novalidate">
                                        <div class="card-body floating-label">
                                            <div class="form-group">
                                                <input type="text" class="form-control" id="Firstname1"
                                                       name="detai" required>
                                                <label for="Firstname1">De tai nghien cuu</label>
                                            </div>
                                            <div class="form-group">
                                                <input type="text" class="form-control" id="Firstname1"
                                                       name="madetai" required>
                                                <label for="Firstname1">Ma de tai Nghien cuu</label>
                                            </div>
                                            <div class="form-group">
                                                <input type="text" class="form-control" id="Firstname1"
                                                       name="mail" required>
                                                <label for="Firstname1">Giảng viên hỗ trợ (Mail giang
                                                    vien)</label>
                                            </div>
                                        </div><!--end .card-body -->
                                        <div class="card-actionbar">
                                            <div class="card-actionbar-row">
                                                <button type="submit"
                                                        class="btn btn-flat btn-primary ink-reaction">Đăng
                                                    kí
                                                </button>
                                            </div>
                                        </div>
                                    </form>
                                    <div> <!-- end tab edit -->
                                    </div>  <!-- endtab -->
                                </div> <!--endcard-->

                                <!--dang ky bao ve-->

                                <div class="tab-pane" id="tab-edit2">
                                    <div class="col-xs-12">
                                        <img class="pull-left width-3" src="/images/gv.gif" alt=""/>
                                        <p>
                                            <a class="text-medium text-lg text-primary" href="#">Dang Ky Bao
                                                Ve</a><br/>
                                            <a class="opacity-75" href="#">uet.vnu.edu.vn</a>
                                        </p>
                                        <div class="contain-xs pull-left">
                                            Ban duoc dang ky bao ve neu ban da dang ky de tai va da duoc giang
                                            vien chap nhan de tai cua ban.
                                            Nop ho so bao ve o duoi
                                        </div>
                                    </div><!--end .col -->
                                    </hr>
                                    </hr>
                                    <div class="about-area">
                                        <h4>choose file</h4>
                                        <div class="row">
                                            <div class="col-xs-12">
                                                <div class="panel panel-default">
                                                    <div class="panel-body">
                                                        <span class="glyphicon glyphicon-cloud-upload"></span>
                                                        <h2>File Uploader</h2>
                                                        <h4>coligo.io</h4>
                                                        <div class="progress">
                                                            <div class="progress-bar" role="progressbar"></div>
                                                        </div>
                                                        <button class="btn btn-lg upload-btn" type="button">
                                                            Upload File
                                                        </button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <input id="upload-input" type="file" name="uploads[]"
                                               multiple="multiple"></br>
                                    </div><!--  end-aboutarea -->

                                    <div> <!-- end tab edit -->
                                    </div>  <!-- endtab -->
                                </div> <!--endcard-->


                                <div class="tab-pane" id="tab-edit">
                                    <form class="form-horizontal" novalidate="novalidate">
                                        <div class="form-group">
                                            <label for="email" class="col-sm-2 control-label">Chuyên
                                                ngành</label>
                                            <div class="col-sm-10">
                                                <input type="chuyennganh" class="form-control" id="chuyennganh"
                                                       required>
                                                <div class="form-control-line"></div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="email" class="col-sm-2 control-label">Email</label>
                                            <div class="col-sm-10">
                                                <input type="email" class="form-control" id="email" required>
                                                <div class="form-control-line"></div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="password"
                                                   class="col-sm-2 control-label">Password</label>
                                            <div class="col-sm-10">
                                                <input type="password" class="form-control" id="password" required>
                                                <div class="form-control-line"></div>
                                            </div>
                                        </div>
                                        <div class="card-actionbar">
                                            <div class="card-actionbar-row">
                                                <button type="submit"
                                                        class="btn btn-flat btn-primary ink-reaction">Save
                                                </button>
                                            </div>
                                        </div>
                                    </form>
                                    <div> <!-- end tab edit -->
                                    </div>  <!-- endtab -->
                                </div> <!--endcard-->
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div><!--end #content-->
    <!-- END CONTENT -->
    <%- include('../menubar'); %>
</div><!--end #base-->
<!-- END BASE -->

<!-- BEGIN JAVASCRIPT -->
<script src="/assets/js/libs/jquery/jquery-1.11.2.min.js"></script>
<script src="/assets/js/libs/jquery/jquery-migrate-1.2.1.min.js"></script>
<script src="/assets/js/libs/bootstrap/bootstrap.min.js"></script>
<script src="/assets/js/libs/nanoscroller/jquery.nanoscroller.min.js"></script>

<script src="/assets/js/libs/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="/assets/js/libs/jquery-validation/dist/additional-methods.min.js"></script>
<script src="/javascripts/uploadhosobaove.js"></script>
<script src="/javascripts/svquanlydetai.js"></script>
<!-- Put App.js last in your javascript imports -->
<script src="/assets/js/core/source/App.min.js"></script>
<!-- <script src="/assets/js/core/source/AppNavigation.js"></script> -->


</body>
</html>
