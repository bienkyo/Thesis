    </head>
    <body class="menubar-hoverable header-fixed ">
        <%- include('../header') %>
        <!-- BEGIN BASE-->
        <div id="base">
             <!-- BEGIN CONTENT-->
            <div id="content">
                    <section>
                    <div class="section-body content-lg">
                        <div class="row">
                            <div class="col-md-3">
                                <div class="card">
                                    <div class="card-body">
                                        <img class="img-circle border-white border-xl img-responsive profile-avatar col-centered" src="/images/gv.gif" alt="">
                                        <h3 class="text-center"><%=user.tenNguoiDung%><br><small>Computer Sicence</small></h3>

                                    </div>
                                </div>
                            </div>
                            <div class="col-md-9">
                                <div class="card">
                                    <div class="card-head">
                                            <ul class="nav nav-tabs" data-toggle="tabs">
                                                <li class="active"><a href="#tab-about">About</a></li>
                                                <li><a href="#tab-edit">Edit</a></li>
                                                <li><a href="#tab-edit1">Dang ky de tai</a></li>
                                            </ul>
                                    </div>
                                    <div class="card-body tab-content">
                                        <div class="tab-pane active" id="tab-about">
                                            <div class="about-area">
                                                <h4 class="text-default-light">Thông tin cá nhân</h4>
                                                <div class="table-responsive">
                                                    <table class="table about-table">
                                                        <tbody>
                                                        <tr>
                                                            <th>Ma sinh vien</th>
                                                            <td><%=nguoihoc.MSSV%></td>
                                                        </tr>
                                                        <tr>
                                                            <th>User Name</th>
                                                            <td><%= user.tenNguoiDung%></td>
                                                        </tr>
                                                        <tr>
                                                            <th>Dia chi mail vnu</th>
                                                            <td><%= user.vnuMail%></td>
                                                        </tr>
                                                        <tr>
                                                            <th>Mat Khau</th>
                                                            <td><%=user.matKhau%></td>
                                                        </tr>
                                                        <tr>
                                                            <th>Dang Ky Khoa Luan</th>

                                                        </tr>
                                                        <tr>
                                                            <th>Chuyên ngành</th>
                                                            <td><%=nguoihoc.nghanhHoc_khNganh%></td>
                                                        </tr>
                                                        <tr>
                                                            <th>Website</th>
                                                            <td><a href="#">UET.vnu.edu.vn</a></td>
                                                        </tr>
                                                        <tr>
                                                            <th>Email</th>
                                                            <td><a href="#">Ctmail.vnu.edu.vn</a></td>
                                                        </tr>
                                                        <tr>
                                                            <th>Phone</th>
                                                            <td>+61 3 8376 6284</td>
                                                        </tr>
                                                        <tr>
                                                            <th>Position</th>
                                                            <td>Xuan Thuy,Cau Giay, Ha Noi</td>
                                                        </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div><!--  end-aboutarea -->
                                        </div><!--  tab-about -->

                                        <div class="tab-pane" id="tab-edit1">
                                            <form class="form" method="post" action="/users/sinhvien/dkdetai">
                                                <div class="card" style="padding: 24px; margin-top: 50px;">
                                                    <div class="card-head style-primary">
                                                        <header>Đăng kí đề tài</header>
                                                    </div>
                                                    <div class="card-body floating-label">
                                                        <div class="form-group">
                                                            <input type="text" class="form-control" id="Firstname1" name="msv">
                                                            <label for="Firstname1">Ma sinh vien</label>
                                                        </div>
                                                        <div class="form-group">
                                                            <input type="text" class="form-control" id="Firstname1" name="detai">
                                                            <label for="Firstname1">De tai nghien cuu</label>
                                                        </div>
                                                        <div class="form-group">
                                                            <input type="text" class="form-control" id="Firstname1" name="madetai">
                                                            <label for="Firstname1">Ma de tai Nghien cuu</label>
                                                        </div>
                                                        <div class="form-group">
                                                            <input type="text" class="form-control" id="Firstname1" name="mail">
                                                            <label for="Firstname1">Giảng viên hỗ trợ (Mail giang vien)</label>
                                                        </div>
                                                        <div class="checkbox checkbox-styled">
                                                            <label>
                                                                <input type="checkbox" value="">
                                                                <span>Send me weekly updates</span>
                                                            </label>
                                                        </div>
                                                    </div><!--end .card-body -->
                                                    <div class="card-actionbar">
                                                        <div class="card-actionbar-row">
                                                            <button type="submit" class="btn btn-flat btn-primary ink-reaction">Đăng kí</button>
                                                        </div>
                                                    </div>
                                                </div><!--end .card -->
                                            </form>
                                            <div> <!-- end tab edit -->
                                            </div>  <!-- endtab -->
                                            <div>
                                                <button>Xin thoi dang ky</button>
                                            </div>
                                        </div> <!--endcard-->

                                        <div class="tab-pane" id="tab-edit">
                                            <form class="form-horizontal">
                                                <div class="form-group">
                                                    <label for="email" class="col-sm-2 control-label">Chuyên ngành</label>
                                                    <div class="col-sm-10">
                                                        <input type="chuyennganh" class="form-control" id="chuyennganh"><div class="form-control-line"></div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="email" class="col-sm-2 control-label">Email</label>
                                                    <div class="col-sm-10">
                                                        <input type="email" class="form-control" id="email"><div class="form-control-line"></div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="password" class="col-sm-2 control-label">Password</label>
                                                    <div class="col-sm-10">
                                                        <input type="password" class="form-control" id="password"><div class="form-control-line"></div>
                                                    </div>
                                                </div>
                                                <div class="card-actionbar">
                                                    <div class="card-actionbar-row">
                                                        <button type="submit" class="btn btn-flat btn-primary ink-reaction">Save</button>
                                                    </div>
                                                </div>
                                            </form>
                                        <div> <!-- end tab edit -->
                                    </div>  <!-- endtab -->
                                </div> <!--endcard-->
                            </div>
                        </div>
                    </div>
                </section>
            </div><!--end #content-->
            <!-- END CONTENT -->
            <%- include('../menubar'); %>
        </div><!--end #base-->
        <!-- END BASE -->

        <!-- BEGIN JAVASCRIPT -->
        <script src="/assets/js/libs/jquery/jquery-1.11.2.min.js"></script>
       <script src="/assets/js/libs/jquery/jquery-migrate-1.2.1.min.js"></script>
       <script src="/assets/js/libs/bootstrap/bootstrap.min.js"></script>
       <script src="/assets/js/libs/nanoscroller/jquery.nanoscroller.min.js"></script>

       <!-- Put App.js last in your javascript imports -->
        <script src="/assets/js/core/source/App.min.js"></script>
        <!-- <script src="/assets/js/core/source/AppNavigation.js"></script> -->


    </body>
</html>
